{
    "name": "php-etl/pipeline",
    "description": "This library implements the Extract-Transform-Load pattern asynchronously in PHP with the help of iterators and generators",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiboko SAS",
            "homepage": "http://kiboko.fr"
        },
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "psr/log": "^1.1",
        "php-etl/bucket": "^0.2.0",
        "php-etl/pipeline-contracts": "^0.3.0",
        "php-etl/bucket-contracts": "^0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "johnkary/phpunit-speedtrap": "*",
        "mybuilder/phpunit-accelerator": "*",
        "phpunit/php-invoker": "*",
        "php-etl/phpunit-extension": "0.4.x-dev"
    },
    "provide": {
        "php-etl/pipeline-implementation": "0.3.0"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\Pipeline\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "unit\\Kiboko\\Component\\ETL\\Pipeline\\": "tests/unit/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.4.x-dev"
        }
    }
}
